

<%= form_with(model: [@quizo, @contestant], url: quizo_contestants_path(@quizo), class: "form") do |form| %>
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.submit 'Create Contestant', class: "btn btn-primary" %>
  </div>
<% end %>

<p style="color: green"><%= notice %></p>

<h1>Contestants:</h1>
<ul>
<% @quizo.contestants.each do |contestant| %>
  <li>
  <%= link_to contestant.name, quizo_contestant_quesitions_path(@quizo, contestant) %>




  </li>
<% end %>



</ul>

<% @quizo.quesitions.each do |quesition| %>
  <% if quesition.available? %>
    <div class="card mb-4">
      <div class="card-body">
        <h5 class="card-title">Question:</h5>
        <p class="card-text">Question ID: <%= quesition.id %></p>
        <%= link_to "View Question", view_quizo_quesition_path(@quizo, quesition, submission_id: @submission.id, contestant_id: @contestant.id), class: "btn btn-primary" %>




      </div>
    </div>
  <% end %>
<% end %>










