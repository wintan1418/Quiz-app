<div class="nested-form-wrapper" data-new-record="<%= f.object.new_record? %>">
  <div class="card mb-4">
    <div class="card-body">
      <%= f.label :name, "Contestant Name" %>
      <%= f.text_field :name, class: "form-control mb-3" %>
      <button type="button" data-action="nested-form#remove">Remove Contestant</button>
      <%= f.hidden_field :_destroy %>
    </div>
  </div>
</div>
